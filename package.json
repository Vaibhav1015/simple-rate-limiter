{
  "name": "node-rate-limiting",
  "version": "1.0.0",
  "description": "A lightweight, dependency-free rate limiter for Node.js using a sliding window",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "type": "commonjs",
  "scripts": {
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "rate-limiter",
    "rate-limiting",
    "api",
    "utility"
  ],
  "author": "Vaibhav Andhale",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vaibhav1015/simple-rate-limiter.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "jest": "^29.7.0",
    "rollup": "^4.9.6"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}